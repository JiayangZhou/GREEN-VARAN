{
    "APPEND_TOOL_DIR": true,
    "BASE_DIR": "resources",
    "REGIONS_DIR": "{BASE_DIR}/bed_files",
    "SCORES_DIR": "{BASE_DIR}/scores",
    "AF_DIR": "{BASE_DIR}/AF",
    "REGIONS": {
        "GREEN_DB": {
            "file": "{REGIONS_DIR}/{build}_GREEN-DB.bed.gz",
            "names": ["NC_regionID","NC_region_type","NC_constraint","NC_median_PhyloP100","NC_closestGene","NC_closestGene_dist", "NC_closestProt", "NC_closestProt_dist","NC_genes","NC_methods"],
            "columns": [4,5,7,8,9,10,11,12,13,14],
            "ops": ["self","self","self","max","uniq","uniq","uniq","uniq","uniq","max"]
        },
        "TFBS": {
            "file": "{REGIONS_DIR}/{build}_TFBS.merged.bed.gz",
            "names": ["NC_TFname"],
            "columns": [4],
            "ops": ["uniq"]
        },
        "UCNE": {
            "file": "{REGIONS_DIR}/{build}_UCNE.sorted.bed.gz",
            "names": ["NC_UCNE"],
            "columns": [3],
            "ops": ["flag"]
        },
        "dbSuper": {
            "file": "{REGIONS_DIR}/{build}_dbSuper.sorted.bed.gz",
            "names": ["NC_dbSUPER"],
            "columns": [3],
            "ops": ["flag"]
        },
        "DNase": {
            "file": "{REGIONS_DIR}/{build}_DNase.merged.bed.gz",
            "names": ["NC_DNase"],
            "columns": [3],
            "ops": ["flag"]
        },
        "LoF_tolerance": {
            "file": "{REGIONS_DIR}/{build}_LoF_tolerance.sorted.bed.gz",
            "names": ["TMPNC_tolerant_P","TMPNC_tolerant_label"],
            "columns": [5,6],
            "ops": ["max","uniq"]
        }
    },
    "SCORES": {
        "FATHMM_MKL" : {
            "file": "{scores_dir}/{build}_FATHMM-MKL_NC.tsv.gz",
            "names": ["NC_FATHMM_MKL"],
            "columns": [5],
            "ops": ["max"]
        },
        "FATHMM_XF" : {
            "file": "{scores_dir}/{build}_FATHMM-XF_NC.tsv.gz",
            "names": ["NC_FATHMM_XF"],
            "ops": ["max"],
            "columns": [5]
        },
        "EIGEN" : {
            "file": "{scores_dir}/{build}_Eigen.tsv.gz",
            "names": ["NC_EIGEN","NC_EIGEN_PC"],
            "ops": ["max","max"],
            "columns": [5,6]
        },
        "GWAVA" : {
            "file": "{scores_dir}/{build}_gwava.bed.gz",
            "names": ["NC_GWAVA"],
            "ops": ["max"],
            "columns": [5]
        },
        "FIRE" : {
            "file": "{scores_dir}/{build}_FIRE.tsv.gz",
            "names": ["NC_FIRE"],
            "ops":["max"],
            "columns": [5]
        },
        "LinSight": {
            "file": "{scores_dir}/{build}_LinSight.bed.gz",
            "names": ["NC_LinSight"],
            "ops":["max"],
            "columns": [4]
        },
        "ExPECTO": {
            "file": "{scores_dir}/{build}_ExPECTO.tsv.gz",
            "names": ["NC_ExPECTO"],
            "ops": ["self"],
            "columns": [8]
        },
        "ReMM": {
            "file": "{scores_dir}/{build}_ReMM.tsv.gz",
            "names": ["NC_ReMM"],
            "ops": ["max"],
            "columns": [3]
        },
        "NCBoost": {
            "file": "{scores_dir}/{build}_NCBoost.tsv.gz",
            "names": ["NC_NCBoost"],
            "ops": ["max"],
            "columns": [5]
        },
        "CADD": {
            "file": "{scores_dir}/{build}_CADD.tsv.gz",
            "names": ["NC_CADD"],
            "ops": ["max"],
            "columns": [5]
        },
        "DANN": {
            "file": "{scores_dir}/{build}_DANN.tsv.gz",
            "names": ["NC_DANN"],
            "ops": ["max"],
            "columns": [5]
        },
        "PhyloP100" : {
            "file": "{scores_dir}/{build}_PhyloP100.bed.gz",
            "names": ["NC_PhyloP100"],
            "ops": ["max"],
            "columns": [4]
        }
    },
    "AF": {
        "file": "{AF_DIR}/{build}_gnomad.genomes.vcf.gz",
        "names": ["gnomAD_AF"],
        "ops": ["self"],
        "fields": ["AF"]
    }
}